{
  "name": "Tracking time with Google Calendar",
  "description": "It allows you to track your time with Google Calendar",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "build/index.html"
  },
  "icons": {
    "16": "build/icons/icon16.png",
    "32": "build/icons/icon32.png",
    "48": "build/icons/icon48.png",
    "128": "build/icons/icon128.png"
  }
}
